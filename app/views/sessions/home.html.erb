<h1>Welcome to California Wineries!</h1>

<div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">SignUp or Login</span>
          <p><%= link_to "Sign Up Here", signup_path %></p>
          <p><%= link_to "Log In Here", login_path %></p>
        </div>
      </div>
    </div>
  </div>
<div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Login with Google</span>
          <p><%= link_to "Log In With Google", '/auth/google_oauth2', method: :post %></p>
        </div>
      </div>
    </div>
  </div>





