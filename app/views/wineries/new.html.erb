<h1>New Winery</h1>

<%= form_for @winery do |f| %>
    
    <%= f.label :name %>
    <%= f.text_field :name %><br><br>

    <%= f.label :website %>
    <%= f.text_field :website %><br><br>

    <%= f.label :phone %>
    <%= f.telephone_field :phone %><br><br>

    <%= f.label :description %>
    <%= f.text_field :description %><br><br>

    <%= f.submit %>

<% end %>